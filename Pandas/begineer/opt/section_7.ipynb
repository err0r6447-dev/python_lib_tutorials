        </span><span><span class="hljs-string">"df['AtRisk'] = np.where((df['PurchaseAmount'] &lt; low_threshold) | (df['Region'].isin(['East'])), 'Yes', 'No')\n"</span></span><span><span class="hljs-punctuation">,</span></span><span>
