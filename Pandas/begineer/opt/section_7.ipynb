    </span><span><span class="hljs-string">"# Detect outliers using Z-score\n"</span></span><span>,
    </span><span><span class="hljs-string">"mean = temp['reading'].mean()\n"</span></span><span>,
    </span><span><span class="hljs-string">"std = temp['reading'].std()\n"</span></span><span>,
    </span><span><span class="hljs-string">"temp['zscore'] = (temp['reading'] - mean) / std\n"</span></span><span>,
