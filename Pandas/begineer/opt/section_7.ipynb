    </span><span><span class="hljs-string">"# Simulate temperature readings\n"</span></span><span>,
    </span><span><span class="hljs-string">"temp = pd.DataFrame({'reading': np.append(np.random.normal(25, 2, 100), [100, -10, 60])})\n"</span></span><span>,
