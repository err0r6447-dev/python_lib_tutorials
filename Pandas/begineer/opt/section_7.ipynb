df = pd.DataFrame({
    </span><span><span class="hljs-string">"user_id"</span></span><span>: np.random.randint(</span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">100_000</span></span><span>, size=N),
    </span><span><span class="hljs-string">"age"</span></span><span>: np.random.randint(</span><span><span class="hljs-number">18</span></span><span>, </span><span><span class="hljs-number">70</span></span><span>, size=N),
    </span><span><span class="hljs-string">"city"</span></span><span>: np.random.choice([</span><span><span class="hljs-string">"New York"</span></span><span>, </span><span><span class="hljs-string">"Paris"</span></span><span>, </span><span><span class="hljs-string">"Berlin"</span></span><span>, </span><span><span class="hljs-string">"Tokyo"</span></span><span>, </span><span><span class="hljs-string">"Delhi"</span></span><span>], size=N),
    </span><span><span class="hljs-string">"spend"</span></span><span>: np.random.uniform(</span><span><span class="hljs-number">10.0</span></span><span>, </span><span><span class="hljs-number">1000.0</span></span><span>, size=N)
})

df.info(memory_usage=</span><span><span class="hljs-string">"deep"</span></span><span>)

