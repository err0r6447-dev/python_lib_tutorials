    </span><span><span class="hljs-string">"\n"</span></span><span>,
    </span><span><span class="hljs-string">"---"</span></span><span>
   ]
  },
  {
   </span><span><span class="hljs-string">"cell_type"</span></span><span>: </span><span><span class="hljs-string">"markdown"</span></span><span>,
   </span><span><span class="hljs-string">"metadata"</span></span><span>: {},
   </span><span><span class="hljs-string">"source"</span></span><span>: [
    </span><span><span class="hljs-string">"##  Real-World Problem 1  Sales KPI Dashboard\n"</span></span><span>,
    </span><span><span class="hljs-string">"\n"</span></span><span>,
    </span><span><span class="hljs-string">"**Scenario:** A retail analytics team wants to identify the **top 3 cities** by total sales and **most popular products per city.**"</span></span><span>
   ]
  },
  {
   </span><span><span class="hljs-string">"cell_type"</span></span><span>: </span><span><span class="hljs-string">"code"</span></span><span>,
   </span><span><span class="hljs-string">"execution_count"</span></span><span>: null,
   </span><span><span class="hljs-string">"metadata"</span></span><span>: {},
   </span><span><span class="hljs-string">"outputs"</span></span><span>: [],
